    </section>
    <section class="featured col" id="featured" role="complementary">
    {{ page.content5 }}
    {{ page.content6 }}
    </section> #}
    </main>
  </div>
</section>

<footer aria-label="Site footer" class="footer" id="footer" role="contentinfo">
   <div class="footer--top">
     {{ page.footer_first }}
     {{ page.footer_second }}
     {{ page.footer_third }}
   </div>
   <div class="footer--bottom">
       {{ page.footer_bottom }}
   </div>
</footer>
